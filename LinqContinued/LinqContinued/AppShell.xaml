<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="LinqContinued.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:LinqContinued"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <Tab Title="Enum">
            <ShellContent ContentTemplate="{DataTemplate local:EnumPage}" />
        </Tab>
        <Tab Title="Select">
            <ShellContent ContentTemplate="{DataTemplate local:SelectionPage}" />
        </Tab>
        <Tab Title="Join">
            <ShellContent ContentTemplate="{DataTemplate local:JoinPage}" />
        </Tab>
    </TabBar>

</Shell>
